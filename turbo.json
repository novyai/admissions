{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "globalEnv": [
    "NODE_ENV",
    "OPENAI_API_KEY",
    "OPENAI_ORG_ID",
    "RW_ENV",
    "IS_LOCAL",
    "WEBHOOK_SECRET",
    "NEXT_PUBLIC_IS_LOCAL",
    "NEXT_PUBLIC_RW_ENV",
    "SERVICE_TOKEN",
    "VERCEL_URL",
    "LOG_LEVEL",
    "REDISPORT",
    "REDISURL",
    "REDISHOST",
    "REDISPASSWORD",
    "REDIS_PRIVATE_URL",
    "CLERK_USER_WEBHOOK_SECRET",
    "PORT"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^db:generate", "^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "start": {},
    "api:start": {},
    "lint": {},
    "test": {
      "cache": false,
      "persistent": false
    },
    "dev": {
      "dependsOn": ["^db:generate", "^db:dev"],
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false,
      "persistent": false
    },
    "type-check": {
      "dependsOn": ["^db:generate"],
      "cache": false,
      "persistent": false
    },
    "db:generate": {
      "cache": false
    },
    "db:format": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:dev": {
      "cache": false
    }
  }
}
